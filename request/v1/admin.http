  "password": "{{PASSWORD}}"
}

### Refresh Admin Token
POST {{BASE_URL}}/{{VERSION}}/admin/login/refresh
x-api-key: {{API_KEY}}
content-type: application/json

{
  "id": "{{ID}}",
  "refreshToken": "{{REFRESH_TOKEN}}"
}

